{
	"name": "@jlbelanger/galleri",
	"version": "1.0.2",
	"description": "Databaseless vanilla JS photo gallery.",
	"license": "GPL-3.0-only",
	"author": {
		"name": "Jenny Belanger",
		"email": "mail@jennybelanger.com",
		"url": "https://www.jennybelanger.com/"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/jlbelanger/galleri.git"
	},
	"scripts": {
		"build": "yarn build:css && yarn build:js",
		"build:css": "node_modules/.bin/gulp css",
		"build:js": "node_modules/.bin/webpack --config webpack.config.js",
		"lint": "yarn lint:css && yarn lint:js",
		"lint:css": "node_modules/.bin/stylelint 'scss/**/*.scss'",
		"lint:js": "node_modules/.bin/eslint . --ext .js",
		"lint:php": "./vendor/bin/phpcs",
		"test:php": "./vendor/bin/phpunit",
		"test:cypress": "node_modules/.bin/cypress open",
		"watch": "yarn watch:js",
		"watch:js": "node_modules/.bin/webpack --config webpack-dev.config.js"
	},
	"dependencies": {
		"normalize.css": "^8.0.1"
	},
	"devDependencies": {
		"autoprefixer": "^10.4.13",
		"cssnano": "^5.1.14",
		"cypress": "^12.2.0",
		"cypress-file-upload": "^5.0.8",
		"eslint": "^8.30.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-plugin-import": "^2.26.0",
		"gulp": "^4.0.2",
		"gulp-cli": "^2.3.0",
		"gulp-postcss": "^9.0.1",
		"gulp-rename": "^2.0.0",
		"gulp-sass": "^5.1.0",
		"sass": "^1.57.1",
		"stylelint": "^14.16.0",
		"stylelint-config-standard-scss": "^6.1.0",
		"stylelint-order": "^5.0.0",
		"stylelint-scss": "^4.3.0",
		"webpack": "^5.75.0",
		"webpack-cli": "^5.0.1"
	}
}
