{
	"name": "jlbelanger/galleri",
	"description": "Databaseless vanilla PHP photo gallery.",
	"version": "1.0.1",
	"type": "project",
	"license": "GPL-3.0-only",
	"authors": [
		{
			"name": "Jenny Belanger",
			"email": "mail@jennybelanger.com",
			"homepage": "https://www.jennybelanger.com/"
		}
	],
	"require": {
		"php": "^8.2"
	},
	"require-dev": {
		"php-mock/php-mock": "^2.4.1",
		"phpunit/phpunit": "^10.5.2",
		"squizlabs/php_codesniffer": "^3.8.0"
	},
	"autoload": {
		"psr-4": {
			"Jlbelanger\\Galleri\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"config": {
		"discard-changes": true
	}
}
